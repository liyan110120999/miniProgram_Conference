/* pages/ImgToText/ImgToText.wxss */
.title_text{
  padding-left: 20px;
  color: #4C9978;
}
.uploadImg_button{
  max-width: 50%;
  margin-top: 40px;
  margin-bottom: 14px;
  border-radius: 30px;
  color: #696969;
  background:linear-gradient(#f2f2f2, #FFF);
  box-shadow: 0 2px 5px 0 rgba(0,0,0,.26);
}
.info_text{
  color: #4C9978;
  font-size: 16px;
}

.uploadImg_button::after{
  border: none;
}
.uploadImg_button_view{
  background-color: #6CD9AA;
  padding-top: 20px;
  padding-bottom: 30px;
  text-align: center;
  box-shadow: 0 2px 5px 0 rgba(0,0,0,.26);
}

.out_text{
  display: inline-block;
  padding-top: 20px;
  color: #696969;
  width: 100%;
  text-align:center;
  /* border-top: 2px solid #696969; */
}
/* 一键复制 */
.copyTBL{
  margin-top: 10px;
  /* margin-bottom: 46px; */
  font-size: 14px;
  padding: 0px 12px;
  line-height:2;
  border-radius: 20px;
  background-color: #fff;
  border:2px solid #6CD9AA;
  color: #6CD9AA;
}
.copyTBL::after{
  border: none;
}

.ad_view{
  background-color: #fff;
  margin: 20px;
  padding: 10px;
  border-radius: 5px;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
}


/* 按钮按压效果 */
.button-hover {
  background-color: #6CD9AA;
  opacity: 0.7;
}

/* 分享给好友 view */
.share_view button {
  background-color:#6CD9AA;
  float:right;
  width: 36px; 
  height: 36px;
  padding: 0;
  margin-right: 3%;
  margin-top: -10px;
}
.share_view button::after{
  border: none;
}
.shareIcon{
  display: block;
  line-height: 36px;
  color: #fff;
  font-size: 18px;
}

